<style>
    .container {
        /*background: gray; */
        min-height: 100vh;
    }
</style>

<div class="container">
    <div class="row">
        <div class="col-md-12">
            <form id="frmFiniquitoUpdate">
           <div class="row mt-3" id="secButtons">
                    <div class="col-md-4" id="buttonsNuevo">
                        <button type="button" onclick="ActualizarFiniquito(this)" class="btn btn-primary" id="Update">Actualizar</button>
                        <button type="button" class="btn btn-primary disabled" id="calcular">Calcular</button>
                        <button type="button" class="btn btn-danger disabled" id="AprobarNom">Aprobar/Nom</button>
                    </div>
                </div>
                <div class="row mt-3">
                    <div class="col-md-4">
                        <div class="form-group">
                            <label for="txtEmpleadoEdit">Empleado</label>
                            <input type="text" class="form-control" id="txtEmpleadoEdit" placeholder="Nombre Empleado" name="txtEmpleadoEdit" disabled />

                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-md-6">
                        <div class="form-group">
                            <label for="txtFFiniquitoEdit">Fecha Finiquito</label>
                            <input type="text" class="form-control" id="txtFFiniquitoEdit" name="txtFFiniquitoEdit" />
                        </div>
                    </div>
                    <div class="col-md-6">
                        <div class="form-group">
                            <label for="txtCompaniaEdit">Compañia</label>
                            <select class="form-control form-control-md" id="txtCompaniaEdit" name="txtCompaniaEdit" disabled>
                                <option selected disabled>--Selecciona--</option>
                            </select>
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-md-6">
                        <div class="form-group">
                            <label for="txtTNominaEdit">Tipo Nomina</label>
                            <select class="form-control form-control-md" id="txtTNominaEdit" name="txtTNominaEdit" disabled>
                                <option selected disabled>--Selecciona--</option>
                            </select>
                        </div>
                    </div>
                    <div class="col-md-6">
                        <div class="form-group">
                            <label for="txtLocalidadEdit">Localidad</label>
                            <select class="form-control form-control-md" id="txtLocalidadEdit" name="txtLocalidadEdit" disabled>
                                @*<option selected disabled>--Selecciona--</option>*@
                            </select>
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-md-6">
                        <div class="form-group">
                            <label for="txtSimulacionEdit">Simulacion</label>
                            <input type="text" class="form-control" id="txtSimulacionEdit" placeholder="Simulación" name="txtSimulacionEdit" />
                        </div>
                    </div>
                    <div class="col-md-6">
                        <div class="form-group">
                            <label for="txtEstatusEdit">Estatus</label>
                            <select class="form-control form-control-md" id="txtEstatusEdit" disabled>
                                <option selected disabled>--Selecciona--</option>
                            </select>
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-md-6">
                        <div class="form-group">
                            <label for="txtPSimulacionEdit">% Simulación</label>
                            <input type="text" class="form-control" id="txtPSimulacionEdit" placeholder="% Simulación" name="txtPSimulacionEdit" />
                        </div>
                    </div>
                    <div class="col-md-6">
                        <div class="form-group">
                            <label for="txtTContratoEdit">Tipo Contrato</label>
                            <select class="form-control form-control-md" id="txtTContratoEdit" name="txtTContratoEdit" disabled>
                                <option selected disabled>--Selecciona--</option>
                            </select>
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-md-6">
                        <div class="form-group">
                            <label for="txtIntIndemEdit">Sue/Integ Indem</label>
                            <input type="text" class="form-control" id="txtIntIndemEdit" placeholder="$0.00" name="txtIntIndemEdit" />
                        </div>
                    </div>
                    <div class="col-md-6">
                        <div class="form-group">
                            <label for="txtIReservadoEdit">Importe Reservado</label>
                            <input type="text" class="form-control" id="txtIReservadoEdit" placeholder="$0.00" name="txtIReservadoEdit" />
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-md-6">
                        <div class="form-group">
                            <label for="txtNLlegarEdit">Neto a llegar</label>
                            <input type="text" class="form-control" id="txtNLlegarEdit" placeholder="$0.00" name="txtNLlegarEdit" />
                        </div>
                    </div>

                    <div class="col-md-6">
                        <div class="form-group">
                            <label for="txtFAntiguedadEdit">Fecha Antiguedad</label>
                            <input type="text" class="form-control" id="txtFAntiguedadEdit"  name="txtFAntiguedadEdit" disabled />
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-md-6">
                        <div class="form-group">
                            <label for="txtSueldoMEdit">Sueldo M</label>
                            <input type="text" class="form-control" id="txtSueldoMEdit" placeholder="$0.00" name="txtSueldoMEdit" disabled />
                        </div>
                    </div>
                    <div class="col-md-6">
                        <div class="form-group">
                            <label for="txtSueldoDEdit">Sueldo D</label>
                            <input type="text" class="form-control" id="txtSueldoDEdit" placeholder="$0.00" name="txtSueldoDEdit" disabled />
                        </div>
                    </div>
                </div>
                <div class="row mt-3">
                    <div class="col-md-12">
                    </div>
                </div>
            </form>
        </div>
    </div>
    <div class="row mt-3">
        <div class="col-md-12">
            <table id="tblNuevoRegistro" class="table table-striped table-bordered dt-responsive nowrap" style="width:100%">
            </table>
        </div>
    </div>
</div>
@section scripts {
<script src="~/js/Finiquito/Index.js"></script>
}